/*Write an anonymous function expression that 
stores a function in a variable called "laugh" and 
outputs the number of "ha"s that you pass in as an argument.*/


var laugh = function(num) {
  var str =""; 
    for (x = 0; x < num; x++){
    str += "ha";   
  }
  return str +"!"; 
};

console.log(laugh(10));

// hahahahahahahahahaha!
